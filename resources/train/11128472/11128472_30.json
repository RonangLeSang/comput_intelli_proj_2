ilder.append(\"/\");\n            builder.append(getWorkType());\n        } else {\n            if (getProgressivoCentro() != null) {\n                builder.append(getProgressivoCentro());\n            }\n            builder.append(\"/\");\n            if (getCommessa() != null) {\n                builder.append(getCommessa());\n            }\n        }\n\n        return builder.toString();\n    }", "signature": "String deriveOrderNumber()", "full_signature": "public String deriveOrderNumber()", "class_method_signature": "OrderProjectImportAdapter.deriveOrderNumber()", "testcase": false, "constructor": false, "invocations": ["getNumero", "append", "toString", "getNumero", "append", "getCentro", "append", "getCentro", "append", "getProgressivoCentro", "getCommessa", "append", "getProgressivoCentro", "append", "append", "getCommessa", "getCommessa", "getWorkType", "append", "getCommessa", "append", "append", "getWorkType", "getProgressivoCentro", "append", "getProgressivoCentro", "append", "getCommessa", "append", "getCommessa", "toString"]}, "repository": {"repo_id": 11128472, "url": "https://github.com/estatio/estatio", "stars": 184, "created": "7/2/2013 3:49:01 PM +00:00", "updates": "2020-01-22T12:30:30+00:00", "fork": "False", "license": "licensed"}}